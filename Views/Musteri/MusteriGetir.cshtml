@model alısveristakip.Models.entity.kullanici
@{
    ViewBag.Title = "MusteriGetir";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@using (Html.BeginForm("Guncelle", "Musteri", FormMethod.Post))
            {
<div class="form-group">
    @Html.LabelFor(m => m.KullanıcıID)
    @Html.TextBoxFor(m => m.KullanıcıID, new { @class = "form-control" })
    <br />
    @Html.LabelFor(m => m.Ad)
    @Html.TextBoxFor(m => m.Ad, new { @class = "form-control" })
    <br />
    @Html.LabelFor(m => m.Soyad)
    @Html.TextBoxFor(m => m.Soyad, new { @class = "form-control" })
</div>
<div>
    <button class="btn btn-warning">Müşteri Güncelle</button>
</div>
            }

